public class Main {
    public static void main(String[] args) {
        int count, sum = 0;
        double avg = 0.0;

        int [] a = { 5, 7 , 9, 45, 24, 5675, 78, 732};
        count = a.length;
    for (int i = 0; i < a.length; i++){
        sum += a[i];
    }
        avg = sum / count;
        System.out.println(avg);
    }
}
