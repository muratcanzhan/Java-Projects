public class Main {
    public static void main(String[] args) {
        int [] list = { 5, 7 , 9, 45, 24, 5675, 78, 732};
        int max = list[0];
        int min = list[0];

        for(int i : list) {
            if (i < min)
                min = i;
            if (i > max)
                max = i;
        }
        System.out.println(" Min : " + min);
        System.out.println(" Max : " + max);
    }
}
